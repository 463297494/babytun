<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
      <mapper namespace="com.wghs.babytun.dao.GoodsCoverDao">
      	<select id="findGoodsCoverById" resultType="com.wghs.babytun.entity.GoodsCover" parameterType="long">
      		select * from t_goods_cover where goods_id = #{id} order by gc_order
      	</select>
      </mapper>  
        
 