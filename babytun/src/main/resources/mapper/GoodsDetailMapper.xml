<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
      <mapper namespace="com.wghs.babytun.dao.GoodsDetailDao">
      	<select id="findGoodsDetailById" resultType="com.wghs.babytun.entity.GoodsDetail" parameterType="long">
      		select * from t_goods_detail where goods_id = #{id} order by gd_order
      	</select>
      </mapper>  
        
 